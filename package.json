{
  "name": "clean-old-jobs",
  "version": "1.2.2",
  "description": "clean k8s old jobs",
  "main": "app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha tests --colors --recursive --exit",
    "test-travis": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec tests --colors --recursive --exit",
    "start": "node app",
    "build": "./dockerfile/build.sh clean-old-jobs"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "keywords": [
    "hkube",
    "hkube-core"
  ],
  "author": "Hkube",
  "license": "MIT",
  "dependencies": {
    "@hkube/config": "^2.0.7",
    "@hkube/etcd": "^5.0.5",
    "@hkube/kubernetes-client": "^1.0.26",
    "@hkube/logger": "^1.0.24",
    "cron": "^1.7.2",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.18.2",
    "express": "^4.17.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^6.2.0",
    "mockery": "^2.1.0",
    "pre-commit": "^1.2.2",
    "sinon": "^7.4.2",
    "uuid": "^3.3.3"
  }
}
